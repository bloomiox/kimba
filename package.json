{
  "name": "kimba",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/build.js",
    "build:vite": "vite build && node scripts/copy-assets.js",
    "verify": "node scripts/verify-build.js",
    "lint": "eslint App.tsx index.tsx components contexts services utils",
    "format": "prettier --write \"{App.tsx,index.tsx,components/**/*.tsx,components/**/*.ts,contexts/**/*.ts,contexts/**/*.tsx,services/**/*.ts,utils/**/*.ts}\"",
    "format:check": "prettier --check \"{App.tsx,index.tsx,components/**/*.tsx,components/**/*.ts,contexts/**/*.ts,contexts/**/*.tsx,services/**/*.ts,utils/**/*.ts}\"",
    "test": "jest",
    "test-urls": "node scripts/test-urls.js",
    "test-translations": "node scripts/test-translations.js",
    "validate-translations": "node scripts/validate-translations.js",
    "test-translation-suite": "node scripts/translation-test-suite.js",
    "pre-commit-check": "node scripts/pre-commit-translation-check.js",
    "test-local": "node scripts/local-server.js",
    "deploy": "npm run build && npm run verify",
    "preview": "vite preview",
    "setup-storage": "node supabase/setup-storage.js",
    "test-storage": "node supabase/test-storage.js"
  },
  "dependencies": {
    "@google/genai": "^1.16.0",
    "@supabase/supabase-js": "2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.3.2",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.46.1",
    "vite": "^6.2.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.52.0",
    "@rollup/rollup-darwin-x64": "^4.52.0",
    "@rollup/rollup-linux-x64-gnu": "^4.52.0",
    "@rollup/rollup-win32-x64-msvc": "^4.52.0"
  }
}
